		IPermit ip = DispatchFactory.createIPermit();
		PermitModel pm = new PermitModel();
		pm.setId(1);
		pm.setName("p1");
		
		ip.create(pm);
		
		PermitModel pm2 = new PermitModel();
		pm2.setId(2);
		pm2.setName("p2");
		
		ip.create(pm2);
		
		List<PermitModel> list = ip.getByCondition(pm, 1,10);
		System.out.println("list=="+list);
	}
}
